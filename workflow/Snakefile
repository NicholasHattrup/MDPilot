# Generate geometries 

rule make_packmol_config:
    input:
        "scripts/pack.inp"
    output:
        "data/system.pdb"
    shell:
        "packmol > {input} " # Generate Packmol file 
        "mv {input} {output}" # Move Packmol generated file to desired file name 

rule make_moltemp_file:
    input:
        "scripts/system.lt"
        "-pdb data/system.pdb"
    output:
        

